<UserControl x:Class="KreasEditor.GameProject.LoadProjectView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KreasEditor.GameProject"
        mc:Ignorable="d" Width="800"
        d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Background="White">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <ListBox Width="190" Height="250"/>
            <Border Width="400" Height="250" Margin="5, 0, 0, 0"/>
        </StackPanel>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0, 50, 0, 0">
            <Button Content="Open" Width="100" IsDefault="True"/>
        </StackPanel>
    </StackPanel>
</UserControl>
